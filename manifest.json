{
	"name":"Extension Practice",
	"version":"1.0",
	"description":"Introduction to chrome extensions",
	"permissions":["tabs","declarativeContent","storage"],
	"background":{
		"scripts":["background.js"],
		"persistent":false
	},
	"browser_action":{
		"default_icon":{
			"16": "images/images/get_started16.png",
        	"32": "images/images/get_started32.png",
        	"48": "images/images/get_started48.png",
        	"128":"images/images/get_started128.png"
		},
		"default_title":"jaja Hola"
	},
	"icons": {
      "16": "images//images/get_started16.png",
      "32": "images//images/get_started32.png",
      "46": "images//images/get_started48.png",
      "128":"images//images/get_started128.png"
    },
	"manifest_version":2
}